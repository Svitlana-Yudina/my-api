{
  "name": "my-api",
  "version": "1.0.0",
  "description": "rest api",
  "main": "dist/app.js",
  "scripts": {
    "start": "nodemon dist/app.js",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline",
    "lint": "eslint . --ignore-path .gitignore",
    "lint-fix": "npm run lint -- --fix",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Svitlana-Yudina/my-api.git"
  },
  "author": "Svitlana Yudina",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Svitlana-Yudina/my-api/issues"
  },
  "homepage": "https://github.com/Svitlana-Yudina/my-api#readme",
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-typescript": "^7.18.6",
    "@mate-academy/eslint-config": "0.0.20",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "nodemon": "^2.0.21",
    "tslint": "^6.1.3",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.24.1",
    "cors": "^2.8.5",
    "express": "^4.18.2"
  }
}
